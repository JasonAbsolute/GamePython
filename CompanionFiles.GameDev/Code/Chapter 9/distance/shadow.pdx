// Distance map
PImage map;

void setup ()
{
  color c;
  size (3200, 2700);
  map = loadImage  ("xx.png");
  for (int i=0; i<3200; i++)
    for (int j=0; j<2700; j++)
    {
      c = map.get(i,j);
      if ((int)red(c) != 255)
        map.set (i,j, color (0,0,0));
      if (i==1 && j==1)
        println (red(c), green(c), blue(c));
    }
}

void draw ()
{
  image(map, 0, 0);
  save("shadow.png");
  exit();
}
